{
  "Serilog": {
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          //文件路径
          "path": ".\\log\\log.log",
          //文件滚动方式
          "rollingInterval": "Day",
          //消息输出格式json
          //"formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
          //文件数量
          "retainedFileCountLimit": 60
        }
      }
    ]
  }
}